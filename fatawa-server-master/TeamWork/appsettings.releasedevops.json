{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MySettings": {
    "MuftiUserId": "#{MuftiUserId}#"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AppConnectionString": "#{connection_string}#"
  },
  "AppSettings": {
    "Token": "#{token}#"
  },
  "StoredFilesPath": "uploaded-files",
  "AllowedClient": "#{allowed_client}#",
  "EmailSettings": {
    "MailServer": "#{email_server}#",
    "MailPort": "#{email_port}#",
    "SenderName": "#{email_sender_name}#",
    "Sender": "#{email_sender}#",
    "Password": "#{email_password}#"
  }
}